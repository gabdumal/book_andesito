{
  "defaultContentLanguage": "pt-BR",
  "languages": {
    "pt-BR": {
      "languageCode": "pt-BR",
      "languageName": "Português (Brasil)",
      "params": {
        "landingPageName": "\u003ci class=\"fa-fw fas fa-home\"\u003e\u003c/i\u003e Início"
      },
      "title": "Ambiente de desenvolvimento",
      "weight": 1
    }
  },
  "markup": {
    "asciidocExt": {
      "attributes": {},
      "backend": "html5",
      "extensions": ["asciidoctor-html5s"],
      "failureLevel": "fatal",
      "noHeaderOrFooter": true,
      "preserveTOC": false,
      "safeMode": "unsafe",
      "sectionNumbers": false,
      "trace": false,
      "verbose": false,
      "workingFolderCurrent": false
    },
    "goldmark": {
      "extensions": {
        "passthrough": {
          "delimiters": {
            "block": [
              ["\\[", "\\]"],
              ["$$", "$$"]
            ],
            "inline": [["\\(", "\\)"]]
          },
          "enable": true
        }
      },
      "parser": {
        "attribute": {
          "block": true
        }
      },
      "renderer": {
        "unsafe": true
      }
    },
    "highlight": {
      "lineNumbersInTable": false,
      "noClasses": false
    }
  },
  "outputs": {
    "home": ["html", "rss", "print"],
    "page": ["html", "rss", "print"],
    "section": ["html", "rss", "print"]
  },
  "params": {
    "additionalContentLanguage": ["en"],
    "author": {
      "email": "professional@gabdumal.com",
      "name": "Gabriel Malosto"
    },
    "disableInlineCopyToClipBoard": true,
    "linkTitle": "amb-dev",
    "themeVariant": [
      {
        "auto": ["zen-light", "zen-dark"],
        "identifier": "zen-auto",
        "name": "Zen Auto"
      },
      {
        "identifier": "zen-light",
        "name": "Zen Claro"
      },
      {
        "identifier": "zen-dark",
        "name": "Zen Escuro"
      }
    ]
  },
  "redirects": [
    {
      "from": "/**",
      "status": 404,
      "to": "/404.html"
    }
  ],
  "security": {
    "enableInlineShortcodes": false,
    "exec": {
      "allow": [
        "^(dart-)?sass(-embedded)?$",
        "^go$",
        "^git$",
        "^npx$",
        "^postcss$",
        "^tailwindcss$",
        "^asciidoctor$"
      ],
      "osEnv": [
        "(?i)^((HTTPS?|NO)_PROXY|PATH(EXT)?|APPDATA|TE?MP|TERM|GO\\w+|(XDG_CONFIG_)?HOME|USERPROFILE|SSH_AUTH_SOCK|DISPLAY|LANG|SYSTEMDRIVE)$"
      ]
    },
    "funcs": {
      "getenv": ["^HUGO_", "^CI$"]
    },
    "http": {
      "methods": ["(?i)GET|POST"],
      "urls": [".*"]
    }
  },
  "theme": "hugo-theme-relearn"
}
